{
  "name": "jalo",
  "version": "0.1.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "client-build": "react-scripts build",
    "preinstall": "node gcp-setup.js",
    "start": "node server.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-build",
    "local-client": "react-scripts start",
    "local-server": "nodemon server.js",
    "dev": "concurrently \"npm run local-server\" \"npm run local-client\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "js-cookie": "^2.2.1",
    "mongoose": "^5.11.11",
    "path": "^0.12.7",
    "query-string": "^6.14.0",
    "react": "^17.0.1",
    "react-datepicker": "^3.4.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "web-vitals": "^0.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
